error && <div className={s.formSummaryError}>{error} </div>}\n                <div>\n                    <button>Login</button>\n                </div>\n            </form>\n    )\n}\nconst LoginReduxForm = reduxForm({form: 'Login'})(LoginForm)\nconst Login = (props) => {\n    const onSubmit = (formData) => {\n        props.login(formData.email, formData.password, formData.remebmerMe)\n    }\n    if (props.isAuth) {\n        return <Navigate to={'/profile'} />\n    }\n    return (\n        <div>\n            <h1>LOGIN</h1>\n            <LoginReduxForm onSubmit={onSubmit}/>\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => ({\n        isAuth: state.auth.isAuth\n})\nexport default connect(mapStateToProps, {login, logout}) (Login);","/Users/konstantin/Documents/Study/my-app/src/components/News/News.jsx",["268"],[],"import React from \"react\";\nimport s from \"./News.module.css\";\n\nconst News = () => {\n    return (\n        <div>\n            News\n        </div>\n    )\n}\n\nexport default News;","/Users/konstantin/Documents/Study/my-app/src/components/Music/Music.jsx",["269"],[],"import React from \"react\";\nimport s from \"./Music.module.css\";\n\nconst Music = () => {\n    return (\n        <div>\n            Music\n        </div>\n    )\n}\n\nexport default Music;","/Users/konstantin/Documents/Study/my-app/src/components/common/preloader/Preloader.js",["270"],[],"import React from 'react';\nimport preloader from '../../../assets/images/preloader.svg';\n\n\nconst Preloader = () => {\n   return (\n    <img src={preloader}/> \n   ) \n}\nexport default Preloader;","/Users/konstantin/Documents/Study/my-app/src/components/Settings/Settings.jsx",["271"],[],"import React from \"react\";\nimport s from \"./Settings.module.css\";\n\nconst Settings = () => {\n    return (\n        <div>\n            Settings\n        </div>\n    )\n}\n\nexport default Settings;","/Users/konstantin/Documents/Study/my-app/src/components/Users/UsersContainer.jsx",["272"],[],"i